msgid "Port"
msgstr "Port"

#: ../jsp/WEB-INF/classes/net/i2p/router/web/jsp/help_jsp.java:653
msgid "Function"
msgstr "Funcţie"

#: ../jsp/WEB-INF/classes/net/i2p/router/web/jsp/help_jsp.java:655
msgid "Notes"
msgstr "Note"

#: ../jsp/WEB-INF/classes/net/i2p/router/web/jsp/help_jsp.java:661
